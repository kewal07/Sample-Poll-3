         else
         {
            $("#main-wrapper").show();
            if (( $(location).attr('pathname') == '/karnataka'|| $(location).attr('pathname') == '/' ) && $.cookie('edition') != 'karnataka' )
            {
               if ($.cookie('edition') == 'karavali' || $.cookie('edition') == 'english' )
                  window.location.assign($.cookie('edition'));
            }             
         }
         
         
         if ($.cookie('edition') == 'karnataka' || $(location).attr('pathname') == '/karnataka')
         {
            $("#karavali").removeClass("selected");
            $("#english").removeClass("selected");
            
            $("#karnataka1").addClass("selected");
         }
          if ($.cookie('edition') == 'karavali' || $(location).attr('pathname') == '/karavali')
         {
            $("#karnataka1").removeClass("selected");
            $("#karavali").addClass("selected");
            $("#english").removeClass("selected");
         }
          if ($.cookie('edition') == 'english' || $(location).attr('pathname') == '/english')
         {
            $("#karnataka1").removeClass("selected");
            $("#karavali").removeClass("selected");
            $("#english").addClass("selected");
         }
         
         $("#karavali1").click(function() {
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'karavali', { expires: 7, path: '/' });              
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div 
              $("#main-wrapper").show();
              $("#karavali").addClass("selected");
              if ($(location).attr('pathname') == '/' || $(location).attr('pathname') == '/kannada')
                  window.location.assign('/karavali');
              
              return false;
         });
            
         $("#english1").click(function() {
              //$.removeCookie('edition', { path: '/' });  
              $.cookie('edition', 'english', { expires: 7, path: '/' });
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div  
              $("#main-wrapper").show();
              $("#english").addClass("selected");
              if ($(location).attr('pathname') == '/' || $(location).attr('pathname') == '/kannada')
               window.location.assign('/english');
              
              return false;
         });
 
         $("#kannada1").click(function() {
               
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'karnataka', { expires: 7, path: '/' });
         
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div 
              $("#main-wrapper").show();  
               $("#karnataka1").addClass("selected"); 
               console.log(" cookie = " + $.cookie('edition'));
               $.cookie('edition', 'karnataka', { expires: 7, path: '/' });   
              return false;
         });
         
         $("#karavali").click(function() {
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'karavali', { expires: 7, path: '/' });
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div 
               $.cookie('edition', 'karavali', { expires: 7, path: '/' });      
console.log(" cookie = " + $.cookie('edition'));               
              window.location.assign('/karavali');
              return false;
         });
            
         $("#english").click(function() {
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'english', { expires: 7, path: '/' });
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div
              $.cookie('edition', 'english', { expires: 7, path: '/' });
              console.log(" cookie = " + $.cookie('edition'));
              window.location.assign('/english');              
              return false;
         });
         
         $("#karnataka1").click(function() {
              $.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'karnataka', { expires: 7, path: '/' });
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div
              $.cookie('edition', 'karnataka', { expires: 7, path: '/' });
              console.log(" cookie = " + $.cookie('edition'));
              window.location.assign('/karnataka');
              return false;
         });
         $("#karavali2").click(function() {
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'karavali', { expires: 7, path: '/' });
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div  
              $.cookie('edition', 'karavali', { expires: 7, path: '/' });     
console.log(" cookie = " + $.cookie('edition'));              
              window.location.assign('/karavali');
              return false;
         });
         
         $("#karnataka2").click(function() {
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'karnataka', { expires: 7, path: '/' });
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div
              $("#main-wrapper").show();
              $.cookie('edition', 'karnataka', { expires: 7, path: '/' });
              console.log(" cookie = " + $.cookie('edition'));
              window.location.assign('/karnataka');
              return false;
         });
         $("#english2").click(function() {
              //$.removeCookie('edition', { path: '/' }); 
              $.cookie('edition', 'english', { expires: 7, path: '/' });
              
              $("#popUpDiv").hide(); // fadein popup div   
              $("#backgroundPopup_1").hide(); // fadein popup div
              $("#main-wrapper").show();
              $.cookie('edition', 'english', { expires: 7, path: '/' });
              console.log(" cookie = " + $.cookie('edition'));
              window.location.assign('/english');
              return false;
         });
         $("#logo").click(function() {
               if (jQuery.isEmptyObject($.cookie('edition')) ||  $.cookie('edition') == 'none')
               {
                    window.location.assign('/karnataka');
                }
               else
               {
                    window.location.assign('/' + $.cookie('edition'));
                 } 
              return false;
         });
         $(".breadcrumb a:first-child").click(function() {
               if (jQuery.isEmptyObject($.cookie('edition')) ||  $.cookie('edition') == 'none')
               {
                    window.location.assign('/karnataka');
                }
               else
               {
                    window.location.assign('/' + $.cookie('edition'));
                 } 
              return false;
         });
         
         $("#admin-menu-icon li a.active:first-child").click(function() {
               if (jQuery.isEmptyObject($.cookie('edition')) ||  $.cookie('edition') == 'none')
               {
                    window.location.assign('/karnataka');
                }
               else
               {
                    window.location.assign('/' + $.cookie('edition'));
                 } 
              return false;
         });
   })(jQuery);
</script>    </div>
  </div>
</div>
